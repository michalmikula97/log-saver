{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "prHourlyLimit": 10,
  "packageRules": [
    {
      "groupName": "All Non-Docker Dependencies",
      "matchDatasources": ["pypi"]
    },
    {
      "groupName": "Docker Image Updates",
      "matchDatasources": ["docker"]
    }
  ]
}